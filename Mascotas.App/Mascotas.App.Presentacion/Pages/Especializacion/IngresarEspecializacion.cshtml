@page
@model Mascotas.App.Presentacion.Pages.IngresarEspecializacionModel
@{
}
<h1>Ingresar Especializaciones </h1>
<form method="post">
    <div class="form-group">
        <label asp-for="Especializaciones.Id"></label>
        <input id="Idn" type="text" asp-for="Especializaciones.Id" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Especializaciones.Aves"></label>
        <input id="Aves" type="text" asp-for="Especializaciones.Aves" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Especializaciones.Perros"></label>
        <input id="Perros" type="text" asp-for="Especializaciones.Perros" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Especializaciones.Gatos"></label>
        <input id="Gatos" type="text" asp-for="Especializaciones.Gatos" class="form-control" />
    </div>
    <div class="form-group">
       <button type="submit" class="btn btn-outline-danger">Ingresar Datos</button>
       <button id="btnCance" onclick="LimpiarFormulario();" type="submit" class="btn btn-outline-danger">Cancelar</button>
       <a asp-page="./ListaEspecializacion" class="btn btn-outline-success">Volver</a>
       </div>     
    </form>